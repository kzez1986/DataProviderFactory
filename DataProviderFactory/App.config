<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<appSettings>
		<add key="provider" value="System.Data.SqlClient" />
	</appSettings>

	<connectionStrings>
		<add name ="AutoLotSqlProvider" connectionString =" Data Source=(local)\; Initial Catalog=AutoLot;Integrated Security=True" />
		<add name="AutoLotOleDbProvider" connectionString="Provider=SQLOLEDB;Data Source=(local)\;Integrated Security=SSPI; Initial Catalog=AutoLot"/>
	</connectionStrings>
	
	<startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
</configuration>